.searchUser{
   width: 670rpx;
   height: 60rpx;
   margin: 10rpx auto;
   padding: 15rpx;
   background: #fff;
   box-shadow: 4rpx 4rpx 20rpx 4rpx rgba(153, 153, 153, 0.17);
   display: flex;
   flex-direction: row;
   align-items: center;
   justify-content: space-between;
}
.searchUser .userInfo{
   width: 400rpx;
   height: 60rpx;
   display: flex;
   flex-direction: row;
   align-items: center;
   justify-content: left;
}
.searchUser .userInfo .avatarUrl{
   width: 50rpx;
   height: 50rpx;
   border-radius: 50%;
}
.searchUser .userInfo .fullname{
   width: 200rpx;
   height: 30rpx;
   line-height: 30rpx;
   margin-left: 20rpx;
   font-size: 24rpx;
   color: #000;
}
.searchUser button{
   width: 136rpx;
   font-size: 16rpx;
}
